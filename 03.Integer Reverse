import java.util.*;

class IntegerReverse
{
public static void main(String[ ] args)
{
Scanner scan = new Scanner(System.in);

int num, sum=0, r;
System.out.println("enter the integr value:");
num =scan.nextInt();

while(num>0)
{
  r = num%10;
  sum = (sum*10)+r;
  num = num/10;
}

System.out.println("The Reverse of the number is"+sum);
}

}
